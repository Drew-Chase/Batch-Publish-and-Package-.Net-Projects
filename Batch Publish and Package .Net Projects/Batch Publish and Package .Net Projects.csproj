	  <EmbeddedResource Include="embedded profiles\Windows ARM Self-Contained.pubxml" />
	  <EmbeddedResource Include="embedded profiles\Windows ARM.pubxml" />
	  <EmbeddedResource Include="embedded profiles\Windows Self-Contained.pubxml" />
	  <EmbeddedResource Include="embedded profiles\Windows.pubxml" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="cclip.net" Version="0.0.1" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Properties\PublishProfiles\" />
	</ItemGroup>

</Project>
